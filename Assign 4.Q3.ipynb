   "source": [
    "def square_numbers(lst):\n",
    "    return list(map(lambda x: x ** 2, lst))\n",
    "\n",
    "sample_list = eval(input(\"enter a list: \"))\n",
    "result = square_numbers(sample_list)\n",
    "\n",
    "print(\"square the elements of the list: \")\n",
    "print(result)"
   ]
